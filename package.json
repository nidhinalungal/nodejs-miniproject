{
  "name": "first-application-vehicle",
  "version": "1.0.0",
  "description": "nodejs training application with APIs",
  "main": "index.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:generate": "npm run typeorm -- migration:generate -d ./ormconfig.ts",
    "typeorm:run": "npm run typeorm -- migration:run -d ./ormconfig.ts",
    "typeorm:revert": "npm run typeorm -- migration:revert -d ./ormconfig.ts"
  },
  "keywords": [
    "node",
    "application"
  ],
  "author": "nidhinalungal",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "inversify": "^6.0.2",
    "inversify-express-utils": "^6.4.6",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.14",
    "pm2": "^5.4.2",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/chai": "^4.2.4",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mocha": "^5.2.7",
    "@types/multer": "^1.4.11",
    "@types/mysql": "^2.15.26",
    "@types/node": "^14.18.63",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "chai": "^4.2.0",
    "mocha": "^9.2.1",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "ts-sinon": "^2.0.2",
    "typescript": "^5.5.3"
  }
}
